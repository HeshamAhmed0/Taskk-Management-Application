@model CreateEmployeeDto
@{
    ViewData["Title"] = "Update";
}

<h1>Update Employee</h1>

<form asp-action="Update" enctype="multipart/form-data">
    <partial name="EmployeePartialView\CreateUpdateEmployee"></partial>
    <input asp-for="ImageName" hidden />

    <div class="my-3">
        <input type="submit" value="Update" class="btn btn-primary" />
        <a asp-action="GetAll" asp-controller="Employee" class="btn btn-secondary">Back To List</a>
    </div>
</form>

